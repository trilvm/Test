<script>
function SaveDraft()
{
	var err = true;
	err = err & validateInput("req",document.frmTraodoi.nguoinhan,"Trường này phải nhập liệu");
    err = err & err==true?validateInput("maxlen=50",document.frmTraodoi.nguoinhan,"Dữ liệu quá dài"):false;                   
    err = err & validateInput("req",document.frmTraodoi.tieude,"Trường này phải nhập liệu");
    err = err & err==true?validateInput("maxlen=100",document.frmTraodoi.tieude,"Dữ liệu quá dài"):false;           
    
    if(err==true)
    {
       	document.getElementById("id_act").value="draft";
		document.frmTraodoi.action = "/traodoi/traodoi/input/";
		document.frmTraodoi.submit();
    }	
	
}
function Send()
{
	document.getElementById("id_act").value="send";
	document.frmTraodoi.action = "/traodoi/traodoi/input/";
	document.frmTraodoi.submit();	
}
function Cancel()
{
	if(trim(document.frmTraodoi.tieude.value)!='' || trim(document.frmTraodoi.nguoinhan.value)!='')
	{
		if(confirm("Bạn chắc chắn muốn hủy tin này? Toàn bộ nội dung thư sẽ bị mất"))
		{
			document.frmTraodoi.action = "/traodoi/traodoi/";
			document.frmTraodoi.submit();		
		}
	}
	else
	{
		document.frmTraodoi.action = "/traodoi/traodoi/";
		document.frmTraodoi.submit();
	}
	
}

function CreateNewMessage()
{
	document.location.href = "/traodoi/traodoi/input";
	
}
function BackButtonClick()
{
	document.frmTraodoi.action = "/traodoi/traodoi/";
	document.frmTraodoi.submit();
}
