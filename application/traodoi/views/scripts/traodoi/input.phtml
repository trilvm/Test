function moveOne(fromList, toList)
	{
		if ( fromList.selectedIndex > -1 )
		{
			var oOption = document.createElement("OPTION");
			
			toList.options.add(oOption);
			oOption.innerText = fromList.options(fromList.selectedIndex).text
