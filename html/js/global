 * @author hoainam12k
 */
var fbcheck = null;
fbcheck = new Image();
fbcheck.src ='http://www.facebook.com/rsrc.php/z2WR4/hash/f17le19o.gif';
var $slidemenu = $("#slidemenu");

jQuery(window).load(function(){

		$("ul#nav").lavaLamp({ fx: "backout", speed: 800, clss: "li.active" });
		//$("ul.pager-list").lavaLamp({ fx: "backout", speed: 800, clss: "li.pager-current" });
  jQuery("img").lazyload({
		placeholder:"http://ntuts.com/themes/vannyneo3/images/box.gif",
		effect:"fadeIn",
		threshhold:200
	});

	$("li.expanded").css({'border': 'none'})
	$("#content .lh h3").hide();
	
	$("#content .lh").hover(function() {
		$(this).addClass("lhhv");
		$(this).append('<div class="lhtrans"></div>');
		$(this).find("h3").show();
	}, function() {
		$(this).removeClass("lhhv");
		$(this).find("h3").hide();
		$(this).find('.lhtrans').remove();
	});
	
	$(".nodelist").hover(function() {
		$(this).addClass("nodelisthv");
	}, function() {
		$(this).removeClass("nodelisthv");
	});
	
	var panel = $("#panel");
	var position = panel.position();
	var pbutton = $("a#pbutton");
	pbutton.click(function() {
		position = panel.position();
		//console.log(position.top);
		if (position.top == 16) {
			panel.animate({'top':'-200'}, 400);
			pbutton.text("[+]");
		} else {
			panel.animate({'top':'16'}, 400);
			pbutton.text("[—]");
		}
		return false;
		//position = panel.position();
	});
	
	$("#slidemenu a").hover(function() {
		$(this).stop().animate({'width':'180px'}, 250);
	}, function() {
		$(this).stop().animate({'width':'40px'}, 250);
	});
	
	//1040
	// Get page sizes
	
	var $smleft = $("#container").offset().left;
	if ($smleft > 50) {
		$smleft1 = $smleft - 50;
		$slidemenu.css({'left': $smleft1 + "px"})
	}
/*
  $('.slideshow').cycle({
		fx: 'fade',
		timeout:  6000
	});
		*/
